KEYCLOAK_USER='{{ keycloak_admin_username }}'
KEYCLOAK_PASSWORD='{{ keycloak_admin_password }}'

# Logging: ALL, DEBUG, ERROR, FATAL, INFO, OFF, TRACE, WARN
KEYCLOAK_LOGLEVEL='{{ keycloak_log_level }}'

# Site
KEYCLOAK_FRONTEND_URL='https://{{ keycloak_domain }}/'
KEYCLOAK_HOSTNAME='{{ keycloak_domain | mandatory }}'
KEYCLOAK_HTTP_PORT='{{ keycloak_app_cont_public_port }}'
PROXY_ADDRESS_FORWARDING='true'
KEYCLOAK_ALWAYS_HTTPS='false'
# Metrics: all/db/http
KEYCLOAK_STATISTICS='all'

# Database
DB_VENDOR='postgres'
DB_ADDR='db'
DB_PORT='{{ keycloak_db_cont_port }}'
DB_USER='{{ keycloak_db_user }}'
DB_PASSWORD='{{ keycloak_db_pass }}'
DB_DATABASE='{{ keycloak_db_name }}'
